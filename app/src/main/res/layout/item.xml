<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="386dp"
        android:layout_height="388dp"
        android:layout_margin="12dp">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="125dp"
            android:layout_height="21dp"
            android:layout_marginStart="48dp"
            android:layout_marginTop="80dp"
            android:text="Born"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/culture" />

        <TextView
            android:id="@+id/born"
            android:layout_width="125dp"
            android:layout_height="20dp"
            android:layout_marginLeft="48dp"
            android:text="born"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="125dp"
            android:layout_height="19dp"
            android:layout_marginStart="48dp"
            android:layout_marginTop="28dp"
            android:text="Culture"
            android:textAlignment="viewStart"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/name" />

        <TextView
            android:id="@+id/culture"
            android:layout_width="125dp"
            android:layout_height="20dp"
            android:layout_marginLeft="48dp"
            android:layout_marginTop="48dp"
            android:text="culture"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/name" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="162dp"
            android:layout_height="25dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="36dp"
            android:text="Aliases"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/titles" />

        <TextView
            android:id="@+id/aliases"
            android:layout_width="162dp"
            android:layout_height="49dp"
            android:layout_marginStart="32dp"
            android:text="aliases"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toEndOf="@+id/born"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="266dp"
            android:layout_height="21dp"
            android:layout_marginStart="48dp"
            android:layout_marginTop="68dp"
            android:text="Played by"
            android:textAlignment="center"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/born" />

        <TextView
            android:id="@+id/playedBy"
            android:layout_width="266dp"
            android:layout_height="41dp"
            android:layout_marginStart="48dp"
            android:text="played by"
            android:textAlignment="center"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="241dp"
            android:layout_height="26dp"
            android:layout_marginStart="48dp"
            android:layout_marginTop="16dp"
            android:text="Name"
            android:textAlignment="center"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/name"
            android:layout_width="241dp"
            android:layout_height="21dp"
            android:layout_marginLeft="48dp"
            android:lineSpacingExtra="8sp"
            android:text="name"
            android:textAlignment="center"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="162dp"
            android:layout_height="20dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="28dp"
            android:text="Titles"
            android:textAlignment="viewStart"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/name" />

        <TextView
            android:id="@+id/titles"
            android:layout_width="162dp"
            android:layout_height="61dp"
            android:layout_marginStart="32dp"
            android:text=" titles"
            android:textAlignment="viewStart"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toEndOf="@+id/culture"
            app:layout_constraintTop_toBottomOf="@+id/textView2" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>